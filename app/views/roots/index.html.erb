                        <div class="summary-wrapper">
                          <div class="rate-wrapper">
                            <div><img src="<%= gem.love_chart_url.untaint %>" width="130" height="50" /></div>
                            <div>愛され度(<%=h gem.result_of_rating[:plus] %>/<%=h gem.result_of_rating[:minus] %> <%=h gem.result_of_rating[:total] %>人による評価)</div>
                          </div>
                          <div class="comments-wrapper">
